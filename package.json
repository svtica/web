  },
  "homepage": "https://github.com/votre-username/ss64-search-pro#readme",
  "devDependencies": {
    "serve": "^14.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "ss64-search-pro.html",
    "js/*.js",
    "README-SS64.md",
    "deploy.js",
    "ss64-maintenance.js",
    "ss64-tests.js"
  ],
