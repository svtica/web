{
  "name": "ss64-search-pro",
  "version": "1.0.0",
  "description": "Recherche avancée hors-ligne pour base de données SS64 avec interface complète",
  "main": "ss64-search-pro.html",
  "scripts": {
    "start": "node deploy.js serve",
    "deploy": "node deploy.js",
    "serve": "python3 -m http.server 8000 || python -m http.server 8000 || npx serve . -p 8000",
    "test": "node ss64-tests.js",
    "validate": "node ss64-maintenance.js validate",
    "stats": "node ss64-maintenance.js stats",
    "optimize": "node ss64-maintenance.js optimize",
    "backup": "node ss64-maintenance.js backup",
    "build": "npm run optimize && npm run validate && npm run deploy",
    "dev": "npm run serve",
    "check": "node deploy.js check",
    "cleanup": "node deploy.js cleanup",
    "report": "node deploy.js report",
    "help": "node deploy.js help"
  },
  "keywords": [
    "ss64",
    "command-line",
    "database",
    "offline",
    "search",
    "windows",
    "linux",
    "macos",
    "powershell",
    "cmd",
    "bash",
    "terminal",
    "documentation",
    "reference"
  ],
